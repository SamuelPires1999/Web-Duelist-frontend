{
  "name": "web-duelist-frontend",
  "description": "The front end app for the web duelist project",
  "private": false,
  "version": "1.1.0",
  "license": "MIT",
  "main": "src/main.tsx",
  "author": "Samuel Pires",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --watch --coverage",
    "test:ui": "vitest --watch --coverage --ui",
    "lint": "npx eslint src",
    "lint:fix": "npm run lint -- --fix",
    "lint:css": "npx stylelint **/*.css --aei --color",
    "prettier": "npx prettier src --check",
    "prettier:fix": "npm run prettier -- --write",
    "format": "npm run prettier:fix && npm run lint:fix",
    "lint-staged": "lint-staged",
    "prepare": "husky install",
    "commit": "commit"
  },
  "dependencies": {
    "@emotion/react": "^11.10.4",
    "@fvilers/disable-react-devtools": "^1.x",
    "@hookform/resolvers": "^2.9.9",
    "@mantine/core": "^5.5.5",
    "@mantine/form": "^5.5.5",
    "@mantine/hooks": "^5.5.5",
    "react": "^18.x",
    "react-dom": "^18.x",
    "react-hook-form": "^7.37.0",
    "react-router-dom": "^6.4.2",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.x",
    "@commitlint/config-conventional": "^17.x",
    "@commitlint/prompt-cli": "^17.x",
    "@testing-library/jest-dom": "^5.x",
    "@testing-library/react": "^13.x",
    "@testing-library/user-event": "^14.x",
    "@trivago/prettier-plugin-sort-imports": "^3.x",
    "@types/react": "^18.x",
    "@types/react-dom": "^18.x",
    "@typescript-eslint/eslint-plugin": "^5.x",
    "@typescript-eslint/parser": "^5.x",
    "@vitejs/plugin-react": "^2.x",
    "@vitest/coverage-c8": "^0.x",
    "@vitest/ui": "^0.x",
    "c8": "^7.x",
    "eslint": "^8.x",
    "eslint-config-prettier": "^8.x",
    "eslint-plugin-import": "^2.x",
    "eslint-plugin-jsx-a11y": "^6.x",
    "eslint-plugin-prettier": "^4.x",
    "eslint-plugin-react": "^7.x",
    "eslint-plugin-react-hooks": "^4.x",
    "happy-dom": "^5.x",
    "husky": "^8.x",
    "lint-staged": "^13.x",
    "msw": "^0.x",
    "prettier": "^2.x",
    "stylelint": "^14.x",
    "stylelint-config-prettier": "^9.x",
    "stylelint-config-standard": "^26.x",
    "typescript": "^4.x",
    "vite": "^3.x",
    "vite-tsconfig-paths": "^3.x",
    "vitest": "^0.x",
    "web-vitals": "^2.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "keywords": [
    "react",
    "vite",
    "typescript"
  ]
}
